{
    "name": "wireui/wireui",
    "description": "TallStack components",
    "type": "library",
    "keywords": [
        "livewire components",
        "livewire-ui",
        "wireui",
        "laravel components",
        "blade components"
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.83|^9.16|^10.0",
        "livewire/livewire": "3.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^6.0|^7.6|^8.0",
        "orchestra/testbench-dusk": "^6.0|^7.6|^8.0",
        "psy/psysh": "@stable",
        "friendsofphp/php-cs-fixer": "^3.9",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "pestphp/pest-plugin-mock": "^2.0"
    },
    "authors": [
        {
            "name": "Pedro Henrique",
            "email": "pedrolivertwd@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WireUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "./vendor/bin/dusk-updater detect --no-interaction"
        ],
        "test": "./vendor/bin/pest",
        "test:ci": "CI=true ./vendor/bin/pest",
        "phpcs": "./vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Providers\\WireUiServiceProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": {
        "livewire-next": {
            "type": "package",
            "package": {
                "name": "livewire/livewire",
                "version": "3.0.0",
                "dist": {
                    "url": "https://calebporzio-public.s3.amazonaws.com/next-main.zip",
                    "type": "zip"
                },
                "autoload": {
                    "files": ["src/helpers.php"],
                    "psr-4": {
                        "Livewire\\": "src/"
                    }
                },
                "extra": {
                    "laravel": {
                        "providers": ["Livewire\\LivewireServiceProvider"],
                        "aliases": {
                            "Livewire": "Livewire\\Livewire"
                        }
                    }
                }
            }
        }
    }
}
